{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-project-covid-reporting"
		},
		"ls_ablob_covid_reporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covid_reporting'"
		},
		"ls_adls_covid_reporting_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid_reporting_dl'"
		},
		"ls_sqlserver_covid_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqlserver_covid'"
		},
		"ls_adls_covid_reporting_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsprojectcovid19.dfs.core.windows.net/"
		},
		"ls_http_opendata_edcd_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_get_file_storage_events_blob_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e1b4fc30-5958-4f94-b314-8d5664a0d531/resourceGroups/rg-reporting-covid/providers/Microsoft.Storage/storageAccounts/saprojectcovidreporting"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-project-covid-reporting_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covid_reporting_connectionString": {
						"value": "[parameters('ls_ablob_covid_reporting_connectionString')]"
					},
					"ls_adls_covid_reporting_dl_accountKey": {
						"value": "[parameters('ls_adls_covid_reporting_dl_accountKey')]"
					},
					"ls_sqlserver_covid_connectionString": {
						"value": "[parameters('ls_sqlserver_covid_connectionString')]"
					},
					"ls_adls_covid_reporting_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid_reporting_dl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_edcd_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_edcd_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-project-covid-reporting_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_get_file_storage_events_blob_properties_typeProperties_scope": {
						"value": "[parameters('tr_get_file_storage_events_blob_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-project-covid-reporting_ArmTemplate_0')]"
			]
		}
	]
}