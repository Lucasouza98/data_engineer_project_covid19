{
	"name": "pl_rest_api",
	"properties": {
		"activities": [
			{
				"name": "copy api to data lake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['data'][0]['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_name']"
								},
								"sink": {
									"name": "employee_name"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_salary']"
								},
								"sink": {
									"name": "employee_salary"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['employee_age']"
								},
								"sink": {
									"name": "employee_age"
								}
							},
							{
								"source": {
									"path": "$['data'][0]['profile_image']"
								},
								"sink": {
									"name": "profile_image"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "ds_rest",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_adls_rest",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "pipeline_restapi"
		},
		"annotations": []
	}
}